  "scripts": {
    "dev": "npm run build:icon && webpack-dev-server --inline --progress --config build/webpack.demo.dev.config.js",
    "start": "npm run dev",
    "unit": "cross-env BABEL_ENV=test karma start test/unit/karma.conf.js --single-run",
    "test": "npm run unit && npm run e2e",
    "build:icon": "node examples/generator.icon.js",
    "build:component": "webpack --config build/webpack.component.config.js",
    "build:umd": "webpack --config build/webpack.umd.config.js",
    "build:scss": "gulp build --gulpfile src/components/themes/gulpfile.js",
    "build:common": "webpack --config build/webpack.common.config.js",
    "build:demo": "npm run build:icon && cross-env NODE_ENV=production  webpack --config build/webpack.demo.prod.config.js",
    "clear": "rm -rf lib",
    "build": "npm run build:scss && npm run build:icon && npm run build:umd && npm run build:common"
  },

{
  "name": "flyer-ui",
  "version": "1.3.0",
  "description": "一款基于Vue框架的UI库",
  "author": "pfzhengd",
  "license": "MIT",
  "main": "lib/flyer-ui.common.js",
  "files": [
    "src",
    "lib"
  ],
  "unpkg": "lib/index.js",
  "style": "lib/themes/index.css",
  "homepage": "https://github.com/flyer-ui/flyer-ui",
  "bugs": {
    "url": "https://github.com/flyer-ui/flyer-ui/issues"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/flyer-ui/flyer-ui.git"
  },
  "scripts": {
    "dev": "webpack-dev-server --inline --progress --config build/webpack.demo.dev.config.js",
    "start": "npm run dev",
    "unit": "cross-env BABEL_ENV=test karma start test/unit/karma.conf.js",
    "test": "npm run unit",
    "build:component": "webpack --config build/webpack.component.config.js",
    "build:umd": "webpack --config build/webpack.umd.config.js",
    "build:scss": "gulp build --gulpfile ./src/components/themes/gulpfile.js",
    "build:common": "webpack --config build/webpack.common.config.js",
    "build:demo": "cross-env NODE_ENV=production  webpack --config build/webpack.demo.prod.config.js",
    "clean": "rm -rf lib",
    "build": "npm run clean && npm run build:scss && npm run build:component && npm run build:umd && npm run build:common",
    "publish:patch": "git checkout master && git merge dev -m 'chore: Merge branch ”dev“' && npm run build && npm version patch && git push --follow-tags && npm publish && git checkout dev",
    "publish:major": "git checkout master && git merge dev -m 'chore: Merge branch ”dev“' && npm run build && npm version major && git push --follow-tags && npm publish && git checkout dev",
    "publish:minor": "git checkout master && git merge dev -m 'chore: Merge branch ”dev“' && npm run build && npm version minor && git push --follow-tags && npm publish && git checkout dev"
  },
  "dependencies": {
    "@flyer-ui/commonality": "0.0.7",
    "popper.js": "^1.15.0"
  },
  "peerDependencies": {
    "vue": ">=2.5.2"
  },
